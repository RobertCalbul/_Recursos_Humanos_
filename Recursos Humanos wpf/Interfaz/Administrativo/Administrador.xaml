<UserControl x:Class="Recursos_Humanos_wpf.Interfaz.Administrador"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="784" d:DesignWidth="702" Width="Auto">

    <Grid Name="pantilla">
        <Grid.RowDefinitions>
            <RowDefinition Height="12*"/>
            <RowDefinition Height="39*"/>
            <RowDefinition Height="733*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150*"/>
            <ColumnDefinition Width="535*"/>
            <ColumnDefinition Width="17*"/>
        </Grid.ColumnDefinitions>
        <TreeView Name="TreeView" Margin="0" Loaded="TreeView_Loaded"
	    SelectedItemChanged="TreeView_SelectedItemChanged" Grid.Row="2"/>
        <Grid Name="WorkSpace" Grid.Column="1" Margin="0" Grid.ColumnSpan="2" Grid.RowSpan="3"/>
        <Label Name="Back" Content="Atras" Margin="0,0,0,5" Grid.Row="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="#FF4185F4" Foreground="White" MouseDown="Label_MouseDown" MouseEnter="Label_MouseEnter" MouseLeave="Label_MouseLeave"/>

    </Grid>
</UserControl>
