<UserControl x:Class="Recursos_Humanos_wpf.Interfaz.Administrador"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="784" d:DesignWidth="702" Width="Auto">

    <Grid Name="pantilla">
        <Grid.RowDefinitions>
            <RowDefinition Height="6*"/>
            <RowDefinition Height="13*"/>
            <RowDefinition Height="373*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150*"/>
            <ColumnDefinition Width="446*"/>
            <ColumnDefinition Width="89*"/>
            <ColumnDefinition Width="17*"/>
        </Grid.ColumnDefinitions>
        <TreeView Name="TreeView" Margin="0" Loaded="TreeView_Loaded"
	    SelectedItemChanged="TreeView_SelectedItemChanged" Grid.RowSpan="3"/>
        <Button Content="Back" Margin="0" Click="back_click" Grid.Column="2" Grid.Row="1" Cursor="Hand"/>
        <Grid Name="WorkSpace" Grid.Column="1" Margin="0" Grid.ColumnSpan="3" Grid.RowSpan="3"/>

    </Grid>
</UserControl>
